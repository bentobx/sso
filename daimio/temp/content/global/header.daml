<!DOCTYPE html> 
<html> 
<head> 
  {%global/metadata | proc | ""}
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="chrome=1">
  <meta name="google-site-verification" content="">
  <meta name="description" content="{@metadata.site.description}">
  <meta name="keywords" content="{@metadata.site.keywords}">
  <meta name="author" content="{@metadata.site.name}">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

  <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
  <link rel="icon" href="/favicon.ico" type="image/x-icon">  

  <title>{@metadata.site.name}: {@metadata.page.title}</title>

  <link rel="stylesheet" href="{@SITE.path}/css/styles.css" type="text/css" media="screen" title="no title" charset="utf-8">
  {begin js | process escape}
    <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-16850490-4', 'soundoffca.ca');
    ga('send', 'pageview');
    </script>
  {end js}  
</head>
